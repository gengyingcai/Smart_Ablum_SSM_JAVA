<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.imooc.demo.dao.AdminDao">
    <select id="selectAdmin" resultType="com.imooc.demo.bo.Admin">
      SELECT *
      FROM admin
      WHERE admin_name=#{name} AND ad_password=#{password}
    </select>
</mapper>